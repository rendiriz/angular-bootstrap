{
  "name": "angular-bootstrap",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./src/config-env.ts",
    "dev": "npm run config -- && ng serve --proxy-config proxy.config.json",
    "build:dev": "npm run config -- --environment=development && ng build --configuration development --output-path=./dist/out",
    "build:prod": "npm run config -- --environment=production && ng build --configuration production --output-path=./dist/out",
    "build:stats": "ng build --stats-json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "dev:ssr": "ng run angular-bootstrap:serve-ssr",
    "serve:ssr": "node dist/angular-bootstrap/server/main.js",
    "build:dev:ssr": "npm run config -- --environment=development && ng build --configuration development && ng run angular-bootstrap:server:development",
    "build:prod:ssr": "npm run config -- --environment=production && ng build --configuration production && ng run angular-bootstrap:server:production",
    "prerender": "ng run angular-bootstrap:prerender",
    "explorer": "source-map-explorer dist/**/*.js --html source-map-explorer.html",
    "analyze": "webpack-bundle-analyzer dist/angular-bootstrap/browser/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.0",
    "@angular/common": "~12.1.0",
    "@angular/compiler": "~12.1.0",
    "@angular/core": "~12.1.0",
    "@angular/forms": "~12.1.0",
    "@angular/platform-browser": "~12.1.0",
    "@angular/platform-browser-dynamic": "~12.1.0",
    "@angular/platform-server": "~12.1.0",
    "@angular/router": "~12.1.0",
    "@emotion/css": "^11.1.3",
    "@fontsource/lato": "^4.4.5",
    "@fontsource/lora": "^4.4.5",
    "@fontsource/roboto": "^4.4.5",
    "@gilsdav/ngx-translate-router": "^3.1.6",
    "@gilsdav/ngx-translate-router-http-loader": "^1.1.2",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0-beta.2",
    "@ng-select/ng-select": "^6.1.0",
    "@ngrx/effects": "^12.2.0",
    "@ngrx/entity": "^12.2.0",
    "@ngrx/store": "^12.2.0",
    "@ngrx/store-devtools": "^12.2.0",
    "@nguniversal/express-engine": "^12.0.0",
    "@ngx-loading-bar/core": "^5.1.2",
    "@ngx-loading-bar/router": "^5.1.2",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^5.0.1",
    "express": "^4.15.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "ngrx-undo": "^8.0.0",
    "ngx-highlight-js": "^12.0.0",
    "ngx-skeleton-loader": "^2.10.0",
    "placeholder-loading": "^0.4.0",
    "rendikit-avatar": "0.0.2",
    "rendikit-theme": "0.0.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "~12.1.0",
    "@angular/localize": "^12.1.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@nguniversal/builders": "^12.0.0",
    "@types/express": "^4.17.0",
    "@types/highlight.js": "^10.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.11.1",
    "babel-plugin-lodash": "^3.3.4",
    "codelyzer": "^6.0.0",
    "dotenv": "^8.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lodash-webpack-plugin": "^0.11.6",
    "moment-locales-webpack-plugin": "^1.2.0",
    "ngx-build-plus": "^12.0.1",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4",
    "webpack-bundle-analyzer": "^4.4.2",
    "yargs": "^16.2.0"
  }
}
